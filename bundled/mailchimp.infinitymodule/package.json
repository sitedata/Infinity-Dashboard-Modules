{
  "name": "MailChimp Stats",
  "description": "Displays the number of subscribers for a specific mailing list",
  "author": "FIPLAB Ltd",
  "website": "https://infinitydashboard.com/",
  "category": "business",
  "bundleID": "com.fiplab.mailchimp",
  "version": "1.0.0",
  "minimumRefreshInterval": 600,


  "user_options": [
    {
      "title": "MailChimp API Key",
      "name": "apiKey",
      "type": "string",
      "required": true
    },

    {
      "title": "MailChimp List ID",
      "name": "listID",
      "type": "string",
      "required": true
    }
  ],

  "dependencies": {
    "mailchimp": "^1.2.1"
  }
}